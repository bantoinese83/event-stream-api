{
  "name": "event-stream-api",
  "version": "1.0.0",
  "description": "Event Stream Analytics API with advanced features",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "npm run start:dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts",
    "test:integration": "jest --config jest.integration.config.ts",
    "test:unit": "jest --config jest.unit.config.ts",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "migrate:dev": "NODE_ENV=development prisma migrate dev",
    "migrate:test": "NODE_ENV=test prisma migrate deploy",
    "migrate:prod": "NODE_ENV=production prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "generate": "prisma generate",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.test.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "env:dev": "cp .env.development .env",
    "env:test": "cp .env.test .env",
    "env:prod": "cp .env.production .env",
    "dev:setup": "npm run env:dev && npm run generate && npm run migrate:dev",
    "test:setup": "npm run env:test && npm run generate && npm run migrate:test",
    "prod:setup": "npm run env:prod && npm run generate && npm run migrate:prod",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:test:build": "docker-compose -f docker-compose.test.yml build",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down -v",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down -v",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "@fastify/auth": "^5.0.2",
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/sensible": "^6.0.3",
    "@google/genai": "^1.2.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-prometheus": "^0.45.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "@prisma/client": "^5.0.0",
    "@timescaledb/core": "^0.0.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.0.0",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "pino": "^8.14.2",
    "pino-pretty": "^10.2.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@testcontainers/postgresql": "^11.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.17.17",
    "@types/node": "^18.0.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-mock-extended": "^3.0.5",
    "msw": "^2.2.3",
    "prettier": "^3.2.5",
    "prisma": "^5.0.0",
    "supertest": "^6.3.4",
    "testcontainers": "^10.7.2",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.0.0"
  }
}
